





import numpy as np
from matplotlib import pyplot as plt

def sinplot(flip=0.5):
   x = np.linspace(0, 14, 100)
   for i in range(1, 5):
      plt.plot(x, np.sin(x + i * .5) * (7 - i) * flip)
    

sinplot()
plt.show()


import seaborn as sns
