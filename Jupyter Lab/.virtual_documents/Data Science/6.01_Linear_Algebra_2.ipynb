# Define function to perform transpose of matrix
def matrix_transpose(x):
    xrows = len(x)
    xcols = len(x[0])
    z = [ [ 0 for i in range(xrows) ] for j in range(xcols) ]
    for i in range(xcols):
        for j in range(xrows):
            z[i][j] = x[j][i]
    return z


