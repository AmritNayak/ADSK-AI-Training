# Define a function to calculate cross product of two vectors
def cross_product(u, v):
    q0 = u[1]*v[2] - u[2]*v[1]
    q1 = u[2]*v[0] - u[0]*v[2]
    q2 = u[0]*v[1] - u[1]*v[0]
    return [q0, q1, q2]

