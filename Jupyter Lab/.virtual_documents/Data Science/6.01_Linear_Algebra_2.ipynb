def determinant_3x3(matrix):
    if len(matrix) == 3 and all(len(row) == 3 for row in matrix):
        a, b, c = matrix[0]
        d, e, f = matrix[1]
        g, h, i = matrix[2]
        return a * (e * i - f * h) - b * (d * i - f * g) + c * (d * h - e * g)
    else:
        return "Matrix must be 3x3"

